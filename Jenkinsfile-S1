pipeline{
  agent any
    environment{
      test= "xy"
    }
  parameters{
    string(name:'pet',defaultvalue:'Jimmy',Description:'Name of my pet')
  }
    agent any
    stages{
      stage('Hello'){
        steps{
          echo 'Environment S1'
          echo '$ my pet name is $pet'
        }
      }
      
    }
}
